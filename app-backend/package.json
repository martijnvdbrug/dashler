{
  "name": "app-backend",
  "version": "0.0.1",
  "description": "backend for dashler frontend",
  "main": "index.js",
  "author": "Martijn van de Brug",
  "license": "MIT",
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@google-cloud/datastore": "^4.3.0",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/graphql": "^6.5.1",
    "@nestjs/platform-express": "^6.7.2",
    "apollo-server-express": "^2.9.3",
    "dotenv": "^8.1.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.8.1",
    "@types/jest": "^24.0.18",
    "copyfiles": "^2.1.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1"
  },
  "scripts": {
    "start": "node dist/index.js",
    "compile": "yarn tsc && yarn copyfiles -u 1 src/**/*.graphql dist && yarn start",
    "dev": "export GOOGLE_APPLICATION_CREDENTIALS=$HOME/.config/gcloud/application_default_credentials.json && export GCLOUD_PROJECT=dasher-253813 && yarn nodemon --delay 0.5 -e ts  --exec \"yarn compile\"",
    "generate": "yarn ts-node src/lib/generate-typings.ts"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
